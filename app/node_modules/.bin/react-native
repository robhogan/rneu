#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/robhogan/workspace/RN0725/node_modules/.pnpm/react-native@0.72.4_@babel+core@7.22.15_@babel+preset-env@7.22.15_react@18.2.0/node_modules/react-native/node_modules:/Users/robhogan/workspace/RN0725/node_modules/.pnpm/react-native@0.72.4_@babel+core@7.22.15_@babel+preset-env@7.22.15_react@18.2.0/node_modules:/Users/robhogan/workspace/RN0725/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/robhogan/workspace/RN0725/node_modules/.pnpm/react-native@0.72.4_@babel+core@7.22.15_@babel+preset-env@7.22.15_react@18.2.0/node_modules/react-native/node_modules:/Users/robhogan/workspace/RN0725/node_modules/.pnpm/react-native@0.72.4_@babel+core@7.22.15_@babel+preset-env@7.22.15_react@18.2.0/node_modules:/Users/robhogan/workspace/RN0725/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/react-native@0.72.4_@babel+core@7.22.15_@babel+preset-env@7.22.15_react@18.2.0/node_modules/react-native/cli.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/react-native@0.72.4_@babel+core@7.22.15_@babel+preset-env@7.22.15_react@18.2.0/node_modules/react-native/cli.js" "$@"
fi
